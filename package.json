{
  "name": "bee-mobile",
  "version": "0.4.3",
  "license": "MIT",
  "description": "UI Components for React",
  "homepage": "https://bee-mobiles.github.io",
  "main": "./index.js",
  "repository": "https://github.com/IronPans/bee-mobile.git",
  "author": "TG <ghmagical@gmail.com>",
  "keywords": [
    "react",
    "bee-mobile",
    "mobile ui",
    "ui",
    "framework"
  ],
  "scripts": {
    "build:dev": "npm run clean:build && cross-env NODE_ENV=development npm run webpack -- --config config/webpack.dev.js --progress --profile",
    "build:prod": "npm run clean:build && cross-env NODE_ENV=production npm run webpack -- --config config/webpack.prod.js  --progress --profile --bail",
    "build": "npm run build:dev",
    "clean:build": "npm run rimraf -- build",
    "lint": "tslint -c tslint.json 'examples/**/*.tsx'",
    "prebuild": "npm run rimraf -- lib && babel ./src/components --source-maps -d ./lib && babel ./src/components/bee-mobile.tsx --source-maps --out-file ./lib/bee-mobile.js",
    "rimraf": "rimraf",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "server:dev": "npm run webpack-dev-server -- --config config/webpack.dev.js --open --progress --profile --watch --content-base examples/",
    "server": "npm run server:dev",
    "start:hmr": "npm run server:dev:hmr",
    "start": "npm run server:dev",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js"
  },
  "dependencies": {
